{
  "defaultSeverity": "error",
  "extends": ["tslint-config-standard", "tslint-config-prettier"],
  "rules": {
    "member-ordering": [true, { "order": "fields-first", "alphabetize": true }],
    "ordered-imports": true,
    "prefer-const": true,
    // "space-before-function-paren": true,
    "trailing-comma": true
  }
}
